        sh:path sbpmnp:expression ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path sbpmnp:parameterRef ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ] .

